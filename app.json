{
  "expo": {
    "name": "Cardy Wall",
    "slug": "cardy-wall",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/cc-round.png",
    "scheme": "cardywall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redonelabs.cardywall"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cc.png",
        "monochromeImage": "./assets/images/credit-card-mono.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.redonelabs.cardywall",
      "permissions": [
        "android.permission.CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "cardywall"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cc.png",
          // "imageWidth": 200,
          // "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}